{
  "name": "react-ssr-squared",
  "version": "0.0.1",
  "description": "React server side rendering w/ extras.",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "build:client": "webpack --progress --colors --config webpack.config.js",
    "build:server": "webpack --progress --colors --config webpack.config.server.js",
    "test": "eslint",
    "build": "npm run build:client && npm run build:server",
    "develop": "export DEV_MODE=\"webpack\" && node app.js",
    "develop:ssr": "nodemon --watch ./dist/server/ --watch ./dist/assets/json/ --delay 2000ms ./dist/server/server.js & webpack --watch --progress --colors --watch-aggregate-timeout 1000 --config webpack.config.server.js & webpack --watch --progress --colors --config webpack.config.js"
  },
  "engines": {
    "node": "6.11.1",
    "npm": "3.10.10"
  },
  "keywords": [
    "React",
    "Server",
    "Side",
    "Rendering",
    "CSS",
    "Modules",
    "Webpack",
    "Code",
    "Splitting"
  ],
  "author": "Charles Rawls",
  "license": "ISC",
  "dependencies": {
    "compression": "^1.7.0",
    "es6-promise": "4.1.1",
    "express": "4.15.3",
    "isomorphic-fetch": "2.2.1",
    "react": "15.6.1",
    "react-dom": "15.6.1",
    "react-helmet": "5.1.3",
    "react-redux": "5.0.5",
    "react-router-dom": "4.1.2",
    "redux": "3.7.2"
  },
  "devDependencies": {
    "assets-webpack-plugin": "3.5.1",
    "babel-core": "6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "7.1.1",
    "babel-plugin-async-to-promises": "1.0.5",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "6.23.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.16.0",
    "css-loader": "0.28.4",
    "esdoc": "0.5.2",
    "eslint": "4.3.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.1.0",
    "extract-text-webpack-plugin": "3.0.0",
    "node-sass": "4.5.3",
    "nodemon": "1.11.0",
    "postcss-cssnext": "^3.0.2",
    "postcss-import": "^10.0.0",
    "postcss-loader": "2.0.6",
    "react-hot-loader": "^3.0.0-beta.7",
    "style-loader": "0.18.2",
    "webpack": "3.3.0",
    "webpack-dev-middleware": "1.11.0",
    "webpack-hot-middleware": "2.18.2",
    "webpack-node-externals": "1.6.0"
  }
}
